{
  "name": "@letsflow/core",
  "version": "1.0.1",
  "description": "Shared library for LetsFlow",
  "author": "Arnold Daniels <arnold@jasny.net>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./scenario": {
      "types": "./lib/scenario/index.d.ts",
      "default": "./lib/scenario/index.js"
    },
    "./process": {
      "types": "./lib/process/index.d.ts",
      "default": "./lib/process/index.js"
    },
    "./schemas/v1.0.0": {
      "types": "./lib/schemas/v1.0.0/index.d.ts",
      "default": "./lib/schemas/v1.0.0/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@letsflow/jmespath": "^1.1.4-jasny.1",
    "@noble/hashes": "^1.4.0",
    "ajv": "^8.16.0",
    "fast-json-stable-stringify": "^2.1.0",
    "get-value": "^3.0.1",
    "mustache": "^4.2.0",
    "set-value": "^4.1.0",
    "uuid": "^10.0.0",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/get-value": "^3.0.5",
    "@types/mocha": "^10.0.7",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.14.9",
    "@types/set-value": "^4.0.3",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "chai": "^4.3.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.5.1",
    "prettier": "^3.3.2",
    "sinon": "^19.0.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "build": "tsc --project tsconfig.build.json",
    "test": "mocha --require ts-node/register 'test/**/*.spec.ts'",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
